{
  "name": "mutant-api-challenge",
  "version": "1.0.0",
  "description": "Challenge para candidato a developer mercadolibre",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "./node_modules/.bin/run-s copy:env:dev build:nodemon",
    "test": "jest",
    "build": "./node_modules/.bin/tsc",
    "prebuild": "rm -Rf dist",
    "build:nodemon": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build:doc": "raml2html -i docs/api.raml -o docs/api.html",
    "postbuild": "npm run copy:all",
    "copy:config": "./node_modules/.bin/cpy 'config/**/*' 'config/**/.*' '!**/*.ts' ./dist/ --no-overwrite --parents",
    "copy:initdb": "./node_modules/.bin/cpy 'src/init-mongo.js' ./dist/ --no-overwrite --parents",
    "copy:env:dev": "./node_modules/.bin/cpy './config/.env.dev' './' --rename='.env' --overwrite",
    "copy:all": "./node_modules/.bin/run-s copy:config copy:initdb"
  },
  "engines": {
    "node": "12.8.3",
    "npm": "6.9.0"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "hapijs",
    "mongodb",
    "docker",
    "jest",
    "raml-documentation",
    "backend"
  ],
  "author": {
    "name": "johnurbaguz",
    "email": "johnurbaguz@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "ISC",
  "dependencies": {
    "@types/base-64": "0.1.3",
    "@types/bluebird": "3.5.32",
    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mongodb": "3.5.27",
    "@types/node": "13.7.7",
    "@types/randomstring": "1.1.6",
    "axios": "0.19.0",
    "base-64": "0.1.0",
    "bcrypt": "5.0.0",
    "bluebird": "3.5.5",
    "body-parser": "1.19.0",
    "cpy-cli": "3.1.0",
    "crypto-js": "4.0.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-joi-validation": "5.0.0",
    "glob": "7.1.6",
    "joi": "17.3.0",
    "jsonwebtoken": "8.5.1",
    "mongodb": "3.6.1",
    "mongodb-autoincrement": "1.0.1",
    "npm-run-all": "4.1.5",
    "randomstring": "1.1.5",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jest": "25.1.4",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "nodemon": "2.0.4",
    "raml2html": "7.6.0",
    "ts-jest": "25.2.1",
    "ts-node": "9.0.0"
  }
}
